{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "one-variable-per-declaration": [
            false
        ],
        "arrow-parens": false,
        "quotemark": [false],
        "object-literal-sort-keys": false,
        "member-ordering": [false],
        "no-string-literal": false,
        "max-line-length": [
            true,
            160
        ]
    },
    "rulesDirectory": []
}
